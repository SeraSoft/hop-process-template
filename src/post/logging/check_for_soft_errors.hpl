    <limit>0</limit>
    <sql>select ec.name event_type
, ile.gravity
, count(*) as num_err 
 from ${integrations.db.log.schema}.integrations_log_events ile 
left outer join ${integrations.db.config.schema}.event_subcategory esc on ile.event_subcategory_id = esc.id
left outer join ${integrations.db.config.schema}.event_category ec on esc.event_category_id = ec.id
inner join ${integrations.db.log.schema}.integrations_logs il on il.id = ile.integrations_logs_id
where il.id = '${v_process_id}'
group by ec.name, ile.gravity</sql>
