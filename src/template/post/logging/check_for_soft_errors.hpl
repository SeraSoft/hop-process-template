    <limit>0</limit>
    <sql>select ec.name category_type
, COALESCE(es.severity, 0) severity
, count(*) as num_err 
 from ${integrations.db.log.schema}.integrations_log_events ile 
left outer join ${integrations.db.config.schema}.events_subcategories esc on ile.event_subcategory_id = esc.id
left outer join ${integrations.db.config.schema}.events_categories ec on esc.event_category_id = ec.id
left outer join ${integrations.db.config.schema}.events_severities es on ile.event_severity_id = es.id
inner join ${integrations.db.log.schema}.integrations_logs il on il.id = ile.integrations_logs_id
where il.id = '${v_process_instance_id}'
group by ec.name, es.severity</sql>
