    <formula>
      <field_name>status</field_name>
      <formula_string>(terminated_successfully.equals("0") ? "E" : (( terminated_successfully.equals("1") &amp;&amp; records_in_error > 0 ) ? "X" : (terminated_successfully.equals("2") ? "L" : (terminated_successfully.equals("3") ? "P" : "T"))))</formula_string>
