<?xml version="1.0" encoding="UTF-8"?>
<pipeline>
  <info>
    <name>Update Hop Process Reference</name>
    <name_sync_with_filename>N</name_sync_with_filename>
    <description/>
    <extended_description/>
    <pipeline_version/>
    <pipeline_type>Normal</pipeline_type>
    <pipeline_status>0</pipeline_status>
    <parameters>
    </parameters>
    <capture_transform_performance>N</capture_transform_performance>
    <transform_performance_capturing_delay>1000</transform_performance_capturing_delay>
    <transform_performance_capturing_size_limit/>
    <created_user>-</created_user>
    <created_date>2020/01/07 10:56:30.129</created_date>
    <modified_user>-</modified_user>
    <modified_date>2020/01/07 10:56:30.129</modified_date>
  </info>
  <notepads>
    <notepad>
      <backgroundcolorblue>112</backgroundcolorblue>
      <backgroundcolorgreen>205</backgroundcolorgreen>
      <backgroundcolorred>255</backgroundcolorred>
      <bordercolorblue>100</bordercolorblue>
      <bordercolorgreen>100</bordercolorgreen>
      <bordercolorred>100</bordercolorred>
      <fontbold>N</fontbold>
      <fontcolorblue>0</fontcolorblue>
      <fontcolorgreen>0</fontcolorgreen>
      <fontcolorred>0</fontcolorred>
      <fontitalic>N</fontitalic>
      <fontname>Segoe UI</fontname>
      <fontsize>9</fontsize>
      <height>88</height>
      <xloc>224</xloc>
      <yloc>368</yloc>
      <note>Process Termination Codes Legenda

T -> Terminated Successfully
E -> Terminated with Errors (Hard Exceptions)
X -> Terminated with ONLY functional errors (Soft Exceptions)
O -> Terminated because another instance of same process is running</note>
      <width>470</width>
    </notepad>
  </notepads>
  <order>
    <hop>
      <from>Start</from>
      <to>Get audit fields</to>
      <enabled>Y</enabled>
    </hop>
    <hop>
      <from>Get audit fields</from>
      <to>Get system info</to>
      <enabled>Y</enabled>
    </hop>
    <hop>
      <from>Get system info</from>
      <to>Set gravity and status</to>
      <enabled>Y</enabled>
    </hop>
    <hop>
      <from>Set gravity and status</from>
      <to>Update Hop process log</to>
      <enabled>Y</enabled>
    </hop>
  </order>
  <transform>
    <name>Get audit fields</name>
    <type>GetVariable</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <fields>
      <field>
        <length>-1</length>
        <name>process_instance_id</name>
        <precision>-1</precision>
        <trim_type>none</trim_type>
        <type>String</type>
        <variable>${v_process_instance_id}</variable>
      </field>
      <field>
        <length>-1</length>
        <name>terminated_successfully</name>
        <precision>-1</precision>
        <trim_type>both</trim_type>
        <type>String</type>
        <variable>${terminated.successfully}</variable>
      </field>
      <field>
        <format>#</format>
        <length>-1</length>
        <name>records_in_error</name>
        <precision>-1</precision>
        <trim_type>none</trim_type>
        <type>Integer</type>
        <variable>${v_err_items_num}</variable>
      </field>
      <field>
        <format>yyyyMMddHHmmss</format>
        <length>-1</length>
        <name>date_start</name>
        <precision>-1</precision>
        <trim_type>both</trim_type>
        <type>Date</type>
        <variable>${v_job_run_timestamp}</variable>
      </field>
    </fields>
    <attributes/>
    <GUI>
      <xloc>325</xloc>
      <yloc>160</yloc>
    </GUI>
  </transform>
  <transform>
    <name>Get system info</name>
    <type>SystemInfo</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <fields>
      <field>
        <name>now</name>
        <type>system date (fixed)</type>
      </field>
    </fields>
    <attributes/>
    <GUI>
      <xloc>490</xloc>
      <yloc>160</yloc>
    </GUI>
  </transform>
  <transform>
    <name>Set gravity and status</name>
    <type>Janino</type>
    <description/>
    <distribute>N</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <formula>
      <field_name>status</field_name>
      <formula_string>(terminated_successfully.equals("0") ? "E" : (( terminated_successfully.equals("1") &amp;&amp; records_in_error > 0 ) ? "X" : (terminated_successfully.equals("2") ? "L" : "T")))</formula_string>
      <value_type>String</value_type>
      <value_length>-1</value_length>
      <value_precision>-1</value_precision>
      <replace_field/>
    </formula>
    <formula>
      <field_name>gravity</field_name>
      <formula_string>terminated_successfully.equals("0") ? 1 : 0</formula_string>
      <value_type>Integer</value_type>
      <value_length>-1</value_length>
      <value_precision>-1</value_precision>
      <replace_field/>
    </formula>
    <attributes/>
    <GUI>
      <xloc>655</xloc>
      <yloc>160</yloc>
    </GUI>
  </transform>
  <transform>
    <name>Start</name>
    <type>RowGenerator</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <fields>
</fields>
    <interval_in_ms>5000</interval_in_ms>
    <last_time_field>FiveSecondsAgo</last_time_field>
    <limit>1</limit>
    <never_ending>N</never_ending>
    <row_time_field>now</row_time_field>
    <attributes/>
    <GUI>
      <xloc>160</xloc>
      <yloc>160</yloc>
    </GUI>
  </transform>
  <transform>
    <name>Update Hop process log</name>
    <type>Update</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <commit>100</commit>
    <connection>${integrations.db.connection.name}</connection>
    <error_ignored>N</error_ignored>
    <lookup>
      <key>
        <condition>=</condition>
        <field>id</field>
        <name>process_instance_id</name>
      </key>
      <schema>${integrations.db.log.schema}</schema>
      <table>integrations_logs</table>
      <value>
        <name>date_finished</name>
        <rename>now</rename>
      </value>
      <value>
        <name>proc_status</name>
        <rename>status</rename>
      </value>
      <value>
        <name>max_gravity</name>
        <rename>gravity</rename>
      </value>
    </lookup>
    <skip_lookup>Y</skip_lookup>
    <use_batch>N</use_batch>
    <attributes/>
    <GUI>
      <xloc>800</xloc>
      <yloc>160</yloc>
    </GUI>
  </transform>
  <transform_error_handling>
  </transform_error_handling>
  <attributes/>
</pipeline>
