    <connection>${serasoft.integrations.db.connection.name}</connection>
    <execute_each_row>N</execute_each_row>
    <limit>0</limit>
    <sql>select 
  ec.name category_type
, esc.name subcategory_type
, COALESCE(es.severity, 0) severity
, source_sys_code source_system
, source_sys_row_ref source_system_reference
, event_text 
, event_data
 from ${serasoft.integrations.db.log.schema}.integrations_log_events ile 
left outer join ${serasoft.integrations.db.config.schema}.events_subcategories esc on ile.event_subcategory_id = esc.id
left outer join ${serasoft.integrations.db.config.schema}.events_categories ec on esc.event_category_id = ec.id
left outer join ${serasoft.integrations.db.config.schema}.events_severities es on ile.event_severity_id = es.id
inner join ${serasoft.integrations.db.log.schema}.integrations_logs il on il.id = ile.integrations_logs_id
where il.id = '${v_process_instance_id}'
</sql>
